{
  "bundleDependencies": false,
  "dependencies": {
    "@material-ui/core": "^1.2.1",
    "lint-staged": "^7.1.3",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.4",
    "react": "^16.4.1",
    "react-do": "^1.1.6",
    "react-dom": "^16.4.1",
    "react-icons-kit": "^1.1.5",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "react-sortable-hoc": "^0.8.3",
    "react-toastify": "^3.4.3",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.1.2",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "deprecated": false,
  "description": "",
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "name": "react-redux-folders-tree",
  "private": false,
  "scripts": {
    "build": "npm-run-all build-css build-js",
    "build-css": "node-sass-chokidar src/Styles -o src/Styles",
    "build-js": "react-scripts build",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "watch-css": "npm run build-css && node-sass-chokidar src/Styles --include-path ./src/Styles --include-path ./node_modules src/Styles/ -o src/Styles --watch --recursive"
  },
  "version": "1.0.1",
  "license": "MIT"
}
